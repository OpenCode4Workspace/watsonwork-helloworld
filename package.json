{
  "name": "watsonwork-sender",
  "description": "A sample app that sends a message to Watson Work",
  "license": "Apache-2.0",
  "version": "0.0.1",
  "homepage": "https://github.com/jsdelfino/watsonwork-sender",
  "bugs": {
    "url": "https://github.com/jsdelfino/watsonwork-sender"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsdelfino/watsonwork-sender.git"
  },
  "keywords": [
    "watson",
    "work",
    "send"
  ],
  "files": [
    ".npmrc",
    "lib/",
    "src/"
  ],
  "main": "lib/app.js",
  "scripts": {
    "sender": "node lib/app.js",
    "babel": "babel src --out-dir lib",
    "test": "mocha lib/test",
    "lint": "eslint src",
    "clean": "rm -rf lib node_modules",
    "build": "npm install && npm run babel && npm run lint && npm test",
    "rebuild": "npm run clean && npm run build",
    "cibuild": "npm run build"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "debug": "^2.2.0",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "chai": "^3.5.0",
    "eslint": "^3.3.0",
    "mocha": "^2.5.3"
  },
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.8"
  }
}
